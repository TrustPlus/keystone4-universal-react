{
  "name": "keystone-beta",
  "version": "0.0.1",
  "description": "Boilerplate for keystone 4 with universal react",
  "private": true,
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-both": "babel -d ./build/both ./both -s",
    "build-server": "babel -d ./build/server ./server -s",
    "build-client": "webpack && cp -a ./public ./build/public",
    "build": "npm run clean && npm run build-client && npm run build-both && npm run build-server",
    "start": "node ./build/server/index.js",
    "mongod": "mongod --dbpath mongo_data/db",
    "dev-start": "babel-node server/index.js",
    "webpack": "webpack -w",
    "nodemon": "nodemon server/index.js --exec babel-node"
  },
  "author": "",
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "react"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  },
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.16.3",
    "html-loader": "^0.4.4",
    "keystone": "^4.0.0-beta.3",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "redux-connect": "^4.0.2",
    "reselect": "^2.5.4",
    "underscore": "^1.8.3",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "css-loader": "^0.25.0",
    "node-sass": "^3.11.2",
    "nodemon": "^1.11.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "nodemon": "^1.11.0",
    "webpack": "^1.13.2"
  }
}
